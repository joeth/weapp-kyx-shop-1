.cp-cityselect{width:100%;background-color:#fff;position:relative;z-index:1000}.cp-cityselect-cancel,.cp-cityselect-confirm{display:block;border:none;outline:none;margin:0;padding:1em 1.5em;height:auto;line-height:inherit;border-radius:0;background-color:transparent;color:#8c8174;font-size:36rpx}.cp-cityselect-cancel:after,.cp-cityselect-confirm:after{display:none}.cp-cityselect-cancel.button-hover,.cp-cityselect-confirm.button-hover{background-color:#f6f1dd}.cp-cityselect-confirm{color:#a91e11}.cp-cityselect-tool{display:flex;align-items:center;justify-content:space-between;background-color:#e9e0cc}.cp-cityselect-view{display:block;height:12em;padding:0 .5em;background-color:white}.cp-cityselect-text{height:100%;line-height:1.5em;padding:0 .5em;display:flex;align-items:center;justify-content:center;text-align:center}